$border: #dddddd;
$feature-background: #f5f5f5;
$menucolor: #bbbbbb;
// main menu hover + inputSubmit:hover
$menuhovercolor: #58a2cb;
// main menu active + footer background color 
$menuactivecolor: #181818;
$button: #dddddd;
$buttonhover: #58a2cb;
$buttonfont: #181818;
$buttonfonthover: #ffffff;

/* ==========================================================================
   Variables
   ========================================================================== */

$footerheight: 160px;

/* ==========================================================================
   Author's custom styles
   ========================================================================== */

/* Main */
html, body{
    height: 100%;
}
body{
    color: #777;
    font-family: 'Open Sans', sans-serif;
}
h1{
    font-family: nexa;
    text-transform: uppercase;
}
h1, h3{
    color: #181818;
}

/* Header */

.mainheader{
    height: 60px;
    -webkit-transition-property: height; 
    -webkit-transition-duration: 0.3s; 
    -webkit-transition-timing-function: linear;
    .headertop, .headerbottom{
        margin-left: 20px;
    }
    .headertop{
        margin-top: 15px;
        overflow: hidden;
        -webkit-transition-property: margin-top; 
        -webkit-transition-duration: 0.3s; 
        -webkit-transition-timing-function: linear;
        -moz-transition-property: margin-top; 
        -moz-transition-duration: 0.3s; 
        -moz-transition-timing-function: linear;
        transition-property: margin-top; 
        transition-duration: 0.3s; 
        transition-timing-function: linear;
        .logo{
            float: left;
            padding-top: 8px;
            width: 47px;
        }
        h1 {
            margin: 0;
            padding-top: 2px;
            display: inline-block;
            float: left;
            a{
                text-decoration: none;
                color: #181818;
            }
        }
    }
    #sub-title{
        font-size: 16px;
        font-family: nexa;
        font-weight: lighter;
        margin: 10px 0 0 0;
        opacity: 0;
        -webkit-transition-property: opacity; 
        -webkit-transition-duration: 0.3s; 
        -webkit-transition-timing-function: linear;
        -moz-transition-property: opacity; 
        -moz-transition-duration: 0.3s; 
        -moz-transition-timing-function: linear;
        transition-property: opacity; 
        transition-duration: 0.3s; 
        transition-timing-function: linear;
    }
    .menu{
        float: left;
        ul{
            padding: 0;
            margin: 0;
            margin-left: 10px;
            padding-top: 12px;
            li {
                list-style: none;
                display: inline-block;
                a{
                    color: $menucolor;
                    -webkit-transition-property: color; 
                    -webkit-transition-duration: 0.1s; 
                    -webkit-transition-timing-function: linear;
                    -moz-transition-property: color; 
                    -moz-transition-duration: 0.1s; 
                    -moz-transition-timing-function: linear;
                    transition-property: color; 
                    transition-duration: 0.1s; 
                    transition-timing-function: linear;  
                }
                &:hover{
                    a{
                        color: $menuhovercolor;
                    }
                }
                &.selected{
                    a{
                        color: $menuactivecolor;
                    }
                }
                a{
                    text-decoration: none;
                    color: $menucolor;
                }
            }
        }
        img{
            width: 20px;
            &#nav{
                cursor: pointer;
                position: absolute;
                right: 15px;
                top: 30px;
                opacity: 0.5;
                &:hover{
                    opacity: 1;
                }
            }
        }
    }
    .lang{
        opacity: 0;
        float: right;
        margin-right: 20px;
        -webkit-transition-property: opacity; 
        -webkit-transition-duration: 0.3s; 
        -webkit-transition-timing-function: linear;
        -moz-transition-property: opacity; 
        -moz-transition-duration: 0.3s; 
        -moz-transition-timing-function: linear;
        transition-property: opacity; 
        transition-duration: 0.3s; 
        transition-timing-function: linear; 
        ul{
            list-style-type: none;
            margin-top: 0;
            margin-bottom: 0;
            -webkit-margin-before: 0;
            -webkit-margin-after: 0;
            li{
                opacity: 0.5;
                float: left;
                margin-left: 10px;
                &.selected{
                    opacity: 1;
                }
                a{
                    display: block;
                }
            }
        }
    }
}

/* Content */

.content{
    .feature {
        padding: 20px;
        border-top: 1px solid $border;
        border-bottom: 1px solid $border;
        background-color: $feature-background;
        p {
            width: 100%;
        }
        .law {
            margin: 0 auto;
            overflow: hidden;
            padding-top: 10px;
            a{
                width: 45%; /* 230/280¬†= 46.43% */
                float: left;
                margin-left: 2.5%;
                margin-right: 2.5%;
                margin-bottom: 20px;
                text-decoration: none;
                img {
                    max-width: 100%;
                }
                .outer{
                    display: table; 
                    position: static;
                    height: 60px;
                    width: 100%;
                    background-color: $button;
                    border-bottom-right-radius: 4px;
                    border-bottom-left-radius: 4px;
                    -webkit-transition-property: background-color; 
                    -webkit-transition-duration: 0.1s; 
                    -webkit-transition-timing-function: linear;
                    -moz-transition-property: background-color; 
                    -moz-transition-duration: 0.1s; 
                    -moz-transition-timing-function: linear;
                    transition-property: background-color; 
                    transition-duration: 0.1s; 
                    transition-timing-function: linear;
                    .center{
                        display: table-cell;
                        vertical-align: middle;
                        width: 100%;
                        position: static;
                        .inner{
                            font-size: 1em;
                            color: $buttonfont;
                            text-align: center;
                            -webkit-transition-property: color; 
                            -webkit-transition-duration: 0.1s; 
                            -webkit-transition-timing-function: linear;
                            -moz-transition-property: color; 
                            -moz-transition-duration: 0.1s; 
                            -moz-transition-timing-function: linear;
                            transition-property: color; 
                            transition-duration: 0.1s; 
                            transition-timing-function: linear;  
                        }
                    }
                }
                &:hover{
                    .outer{
                        background-color: $buttonhover;
                        .center{
                            .inner{
                                color: $buttonfonthover;
                            }
                        }
                    }
                }
            }
        }
    }
    .home-content{
        margin: 0 20px;
        padding: 15px 0;
        p{
            font-size: 14px;
            line-height: 24px;
        }
    }
}
.law-content {
    margin: 0 20px;
}
.faded {
    opacity: .5;
}

/* Team */

.team {
    //text-align: center;
    padding-bottom: 50px;
}

figure {
    display: inline-block;
    padding: 0px 26px;
    width: 105px;
    margin-bottom: 10px;

    img {
        width: 105px;
    }
    figcaption {
        display: table;
        font-size: 12px;
        text-align: center;
        width: inherit;
        margin-top: 10px;
    }
}

.expander {
    background: $feature-background;
    width: 100%;
    margin-bottom: 6px;
    float: left;
    padding: 0 0 20px 20px;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    -ms-box-sizing: border-box;
    box-sizing: border-box;
}


/* Contact */

.contact{
    div{

        h3{
            margin-left: 20px;
        }
        p{
            margin-left: 20px;
        }
    }
}
.location {
    padding: 20px;
    border-bottom: 1px solid #ddd;
    p{
        display: inline-block;
        vertical-align: top;
        margin-right: 40px;
    }
    img {
        display: inline-block;
        margin-top: 10px;
        width: 106px;
    }
}
.mod {
    padding-left: 20px;
}

input.inputText, input.inputPassword, input.inputSearch, input.inputDate, textarea {
    //font-family: "Crimson Text", georgia, serif;
    font-size: 0.8em;
    background-color: #FFFFFF;
    border: 0 none;
    border: 1px solid #CCC;
    color: #666666;
    margin: 0;
    padding: 0.75em 0;
    width: 95%;
    text-indent: 1em;
    border-radius: 2px;
    outline: none;
}

textarea {
    line-height: 1.8;
    resize: none;
}

input.inputText:focus, input.inputPassword:focus, input.inputSearch:focus, input.inputDate:focus, textarea:focus {
    -webkit-box-shadow: 0 0 5px 0 rgba(0,0,0,0.2);
      -moz-box-shadow: 0 0 5px 0 rgba(0,0,0,0.2);
       -ms-box-shadow: 0 0 5px 0 rgba(0,0,0,0.2);
        -o-box-shadow: 0 0 5px 0 rgba(0,0,0,0.2);
           box-shadow: 0 0 5px 0 rgba(0,0,0,0.2);
}

.inputSubmit {
    margin:0 0 1.3em 0;
    padding: 0.7em 1em;
    font-size: 0.9em;
    //font-family: 'Oswald', arial, sans-serif;
    font-weight: normal;
    line-height: 2em;
    text-transform: uppercase;
    background: #181818;
    border: none;
    color: #fff;
    -webkit-border-radius: 2px;
      -moz-border-radius: 2px;
       -ms-border-radius: 2px;
        -o-border-radius: 2px;
           border-radius: 2px;
    cursor: pointer;
    -webkit-transition-property: background-color; 
    -webkit-transition-duration: 0.2s; 
    -webkit-transition-timing-function: linear;
    -moz-transition-property: background-color; 
    -moz-transition-duration: 0.2s; 
    -moz-transition-timing-function: linear;
    transition-property: background-color; 
    transition-duration: 0.2s; 
    transition-timing-function: linear;
}

.inputSubmit:hover {
    background-color: $menuhovercolor;
}

label {
    display: block;
    font-size: 0.8em;
    letter-spacing: 0.1em;
    line-height: 2.25em;
    margin-bottom: 0;
    list-style: none outside none;
}


.warning,
.formError {
    color: red;
}

.success {
    color: green;
}

/* Footer */

footer {
    background-color: $menuactivecolor;
    .footer-content{
        margin-left: 20px;
    }
    a, h3, p, li{
        font-size: 12px;

    }
    a{
        color: #777;
        text-decoration: none;
    }
    h3 {
        color: #e5e5e5;
    }
    p {
        color: #666;
    }
    ul {
        padding: 0;

        li {
            list-style: none;
            color: #666;
        }
    }
    .links {
        li {
            display: inline-block;
            &:after {
                content: ' |';
            }
            &:last-child{
                &:after {
                    content: '';
                }
            }
        }
    }
    div {
        display: inline-block;
        vertical-align: top;
        margin-right: 80px;
        &:last-child{
            margin-right: 0;
        }
    }
}

/* ==========================================================================
   EXAMPLE Media Queries for Responsive Design.
   These examples override the primary ('mobile first') styles.
   Modify as content requires.
   ========================================================================== */

@media only screen and (min-width: 520px) {
    .mainheader{
        .headertop{
            h1{
            } 
        }
        .headerbottom{
            #sub-title{
                margin-top: 0px;
            }
        }
    }

    footer{
        .footer-content{
            width: 400px;
        }
    }
    .content{
        .feature {
            p {
                //width: 80%;
            }
            .law{
                a{
                    width: 215px;
                    img{

                    }
                }
            }
        }
    }
    .contact {
        div {
            display: inline-block;
            //width: 200px;
        }
    }
}

@media only screen and (min-width: 640px) {
    .mainheader {
        height: 100px;
        .headertop{
            margin-top: 30px;
            h1{
                display: block !important;
            }
        }
        .headerbottom{
            #sub-title{
                opacity: 1;
            }
            .lang{
                opacity: 1;
            }
        }
        .menu {
            text-align: center;
            float: right;
            margin-right: 20px;
            .mainnav{
                opacity: 1
            }
            ul {
                margin: 0;
                li{
                    margin-right: 15px;
                    margin-left: 50px;
                    &:last-child{
                        margin-right: 0;
                    }
                }
            }
        }
        h2#sub-title{
            display: block;
        }
    }
    .mainnav {
        display: block !important;
    }
    #nav {
        display: none;
    }
    footer {
        //padding: 20px 60px;
        .footer-content{
            //border: 1px solid red;
        }
    }
    .feature {
        p {
            width: 80%;
        }
    }
    .location {
        width: 20%;
        display: inline-block;
        vertical-align: top;
        padding: 0;
        padding-left: 2.5%;
        padding-top: 2.5%;
        border-bottom: none;
        p{
            display: block;
            vertical-align: top;
            margin-right: 0px;
            margin: 0;
        }
        img {
            display: block;
            margin-top: 10px;
            width: 106px;
        }
    }
    .contact{
        border-bottom: 1px solid #ddd;
        border-left: 1px solid #ddd;
        display: inline-block;
        padding-left: 2.5%;
        width: 74.38%;
        div {
            display: inline-block;
            //margin-right: 100px;
            width: 200px;
        }
    }
    footer{
        .footer-content{
            //border: 1px solid green;
            width: 95%;
        }
    }
}

@media print,
       (-o-min-device-pixel-ratio: 5/4),
       (-webkit-min-device-pixel-ratio: 1.25),
       (min-resolution: 120dpi) {
    /* Style adjustments for high resolution devices */
}

/* ==========================================================================
   Print styles.
   Inlined to avoid required HTTP connection: h5bp.com/r
   ========================================================================== */

@media print {
    * {
        background: transparent !important;
        color: #000 !important; /* Black prints faster: h5bp.com/s */
        box-shadow: none !important;
        text-shadow: none !important;
    }

    a,
    a:visited {
        text-decoration: underline;
    }

    a[href]:after {
        content: " (" attr(href) ")";
    }

    abbr[title]:after {
        content: " (" attr(title) ")";
    }

    /*
     * Don't show links for images, or javascript/internal links
     */

    .ir a:after,
    a[href^="javascript:"]:after,
    a[href^="#"]:after {
        content: "";
    }

    pre,
    blockquote {
        border: 1px solid #999;
        page-break-inside: avoid;
    }

    thead {
        display: table-header-group; /* h5bp.com/t */
    }

    tr,
    img {
        page-break-inside: avoid;
    }

    img {
        max-width: 100% !important;
    }

    @page {
        margin: 0.5cm;
    }

    p,
    h2,
    h3 {
        orphans: 3;
        widows: 3;
    }

    h2,
    h3 {
        page-break-after: avoid;
    }
