$border: #dddddd;
$feature-background: #f5f5f5;
$menucolor: #bbbbbb;
$menuhovercolor: #58a2cb;
$menuactivecolor: #181818;

/* ==========================================================================
   Author's custom styles
   ========================================================================== */


/* Main */

body {
    color: #777;
    font-family: 'Open Sans', sans-serif;
}

h1, h3 {
    color: #181818;
}

.home-content{
    margin-left: 20px;
}

.feature {
    //text-align: center;
    padding: 20px;
    border-top: 1px solid $border;
    border-bottom: 1px solid $border;
    background-color: $feature-background;

    p {
        width: 100%;
    }

    a {
        width: 45%;
        
        &:nth-child(odd){
            margin-right: 24px;
        }

        img {
            width: 45%;
        }
    }
}

.faded {
    opacity: .5;
}

/* Team */

.team {
    //text-align: center;
}

figure {
    display: inline-block;
    padding: 0px 26px;
    width: 105px;

    img {
        width: 105px;
    }
    figcaption {
        display: table;
    }
}

.expander {
    background: #181818;
    //height: 100px;
    width: 100%;
    margin-bottom: 6px;
    float: left;
}

/* Header */

header{

    .logo{
        float: left;
        //padding: 20px;
        padding-top: 8px;
        padding-left: 20px;
    }

    h1 {
        margin: 0;
        //margin-left: 20px;
        a{
            text-decoration: none;
        }
    }

    #sub-title{
        display: none;
    }

    nav{
        ul{
            padding: 0;
            margin: 0;
            margin-left: 20px;

            li {
                list-style: none;
                display: inline-block;
                color: $menucolor;

                &:hover{
                    a{
                        color: $menuhovercolor;
                    }
                }

                &.selected{
                    a{
                        color: $menuactivecolor;
                    }
                }
                a{
                    text-decoration: none;
                    color: $menucolor;
                }
            }
        }
        &.menu{
            img{

                width: 20px;
                &#nav{
                    position: absolute;
                    right: 7px;
                    top: 11px;
                }
            }
            ul{
                li{
                    a{

                    }
                }
            }
        }
        &.lang{
            display: none;
        }
        
    }
}

/* Contact */

.location {
    padding: 20px;
    border-bottom: 1px solid #ddd;
    p{
        display: inline-block;
        vertical-align: top;
        margin-right: 40px;
    }
    img {
        display: inline-block;
        margin-top: 10px;
        width: 106px;
    }
}

.mod {
    padding-left: 20px;
}

input.inputText, input.inputPassword, input.inputSearch, input.inputDate, textarea {
    //font-family: "Crimson Text", georgia, serif;
    font-size: 0.8em;
    background-color: #FFFFFF;
    border: 0 none;
    border: 1px solid #CCC;
    color: #666666;
    margin: 0;
    padding: 0.75em 0;
    width: 95%;
    text-indent: 1em;
    border-radius: 2px;
    outline: none;
}

textarea {
    line-height: 1.8;
    resize: none;
}

input.inputText:focus, input.inputPassword:focus, input.inputSearch:focus, input.inputDate:focus, textarea:focus {
    -webkit-box-shadow: 0 0 5px 0 rgba(0,0,0,0.2);
      -moz-box-shadow: 0 0 5px 0 rgba(0,0,0,0.2);
       -ms-box-shadow: 0 0 5px 0 rgba(0,0,0,0.2);
        -o-box-shadow: 0 0 5px 0 rgba(0,0,0,0.2);
           box-shadow: 0 0 5px 0 rgba(0,0,0,0.2);
}

.inputSubmit {
    margin:0 0 1.3em 0;
    padding: 0.7em 1em;
    font-size: 0.9em;
    //font-family: 'Oswald', arial, sans-serif;
    font-weight: normal;
    line-height: 2em;
    text-transform: uppercase;
    background: #181818;
    border: none;
    color: #fff;
    -webkit-border-radius: 2px;
      -moz-border-radius: 2px;
       -ms-border-radius: 2px;
        -o-border-radius: 2px;
           border-radius: 2px;
    cursor: pointer;

}

.inputSubmit:hover {
    background-color: #5bb75b;
}

label {
    display: block;
    font-size: 0.8em;
    letter-spacing: 0.2em;
    line-height: 2.25em;
    margin-bottom: 0;
    list-style: none outside none;
    text-transform: uppercase;
}


.warning,
.formError {
    color: red;
}

.success {
    color: green;
}

/* Footer */

footer {
    background-color: #181818;
    padding: 0 20px;
    
    a{
        color: #777;
        text-decoration: none;
    }

    h3 {
        color: #e5e5e5;
    }

    p {
        color: #666;
    }

    ul {
        padding: 0;

        li {
            list-style: none;
            color: #666;
        }
    }

    .links {
        li {
            display: inline-block;
            &:after {
                content: ' |';
            }
            &:last-child{
                &:after {
                    content: '';
                }
            }
        }
    }

    div {
        display: inline-block;
        vertical-align: top;
        margin-right: 80px;
        &:last-child{
            margin-right: 0;
        }
    }
}

/* ==========================================================================
   EXAMPLE Media Queries for Responsive Design.
   These examples override the primary ('mobile first') styles.
   Modify as content requires.
   ========================================================================== */

@media only screen and (min-width: 321px) {
    #sub-title{
        display: block;
    }
    .mainnav {
        display: block !important;
    }
    #nav {
        display: none;
    }
    footer {
        //padding: 20px 60px;
        .footer-content{
            //border: 1px solid red;
        }
    }
}

@media only screen and (min-width: 520px) {
    footer{
        .footer-content{
            //border: 1px solid orange;
            width: 400px;
        }
    }
}

@media only screen and (min-width: 640px) {
    .feature {
        p {
            width: 80%;
        }
    }

    footer{
        .footer-content{
            //border: 1px solid green;
            width: 100%;
        }
    }
}

@media print,
       (-o-min-device-pixel-ratio: 5/4),
       (-webkit-min-device-pixel-ratio: 1.25),
       (min-resolution: 120dpi) {
    /* Style adjustments for high resolution devices */
}

/* ==========================================================================
   Print styles.
   Inlined to avoid required HTTP connection: h5bp.com/r
   ========================================================================== */

@media print {
    * {
        background: transparent !important;
        color: #000 !important; /* Black prints faster: h5bp.com/s */
        box-shadow: none !important;
        text-shadow: none !important;
    }

    a,
    a:visited {
        text-decoration: underline;
    }

    a[href]:after {
        content: " (" attr(href) ")";
    }

    abbr[title]:after {
        content: " (" attr(title) ")";
    }

    /*
     * Don't show links for images, or javascript/internal links
     */

    .ir a:after,
    a[href^="javascript:"]:after,
    a[href^="#"]:after {
        content: "";
    }

    pre,
    blockquote {
        border: 1px solid #999;
        page-break-inside: avoid;
    }

    thead {
        display: table-header-group; /* h5bp.com/t */
    }

    tr,
    img {
        page-break-inside: avoid;
    }

    img {
        max-width: 100% !important;
    }

    @page {
        margin: 0.5cm;
    }

    p,
    h2,
    h3 {
        orphans: 3;
        widows: 3;
    }

    h2,
    h3 {
        page-break-after: avoid;
    }
